import java.util.Scanner;
class neon

{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.print("Enter a number to check it is neon number or not ");
		int x=s.nextInt();
		
		neon(x);
	}
	public static void neon(int x) 
	{    
		int a=x;
			int c=x*x;
			int d=0;
			while (c>0)
			{
				int e=c%10;
				d+=e;
				c=c/10;
			}
			if (d==x)
		{
			System.out.println(x+"it is neon number");
		}
		else 
		{
			System.out.println(x+"it is not neon number");
		}
		
	}
}
