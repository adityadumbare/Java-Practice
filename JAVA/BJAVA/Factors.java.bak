import java.util.Scanner;
class Factors 
{
	public static void main(String[] args) 
	{
		
		
		Scanner s=new Scanner(System.in);
		System.out.print("Enter number of * pattern to print ");
		int a=s.nextInt();
		//System.out.println(Factors(a));
		System.out.println("Number of count for factors are "+Factors(a));
	}

	public static int Factors(int a)
	{
		int count=1;
		int b=1;
        for (int i=1;i<=a ;i++ )
        {
			if(a%i==0)
			{
				System.out.println(b++ +"factors of "+a+"are "+i);
				count++;
			}

        }
		return count;
	}
}
